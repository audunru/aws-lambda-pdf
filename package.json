{
  "name": "aws-lambda-pdf",
  "version": "0.2.1",
  "description": "Create PDFs using Amazon Lambda functions",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/index.js",
    "dist/index.d.ts"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "esbuild src/index.ts --outfile=dist/index.js --bundle --platform=node --target=node18 --packages=external",
    "postbuild": "tsc",
    "lint": "eslint src",
    "format": "prettier --write src",
    "prepare": "husky",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "playwright-aws-lambda": "^0.11.0",
    "playwright-core": "^1.48.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.13.0",
    "@playwright/test": "^1.48.1",
    "@types/aws-lambda": "^8.10.145",
    "@types/eslint__js": "^8.42.3",
    "@types/node": "^18.19.57",
    "aws-lambda": "^1.0.7",
    "aws-sdk": "^2.1691.0",
    "esbuild": "^0.24.0",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "npm-run-all": "^4.1.5",
    "pdfjs-dist": "^4.7.76",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.10.0",
    "vitest": "^2.1.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/audunru/aws-lambda-pdf.git"
  },
  "keywords": [
    "aws",
    "lambda",
    "pdf"
  ],
  "bugs": {
    "url": "https://github.com/audunru/aws-lambda-pdf/issues"
  },
  "homepage": "https://github.com/audunru/aws-lambda-pdf",
  "engines": {
    "node": "^18.0.0",
    "npm": ">=10.0.0"
  }
}
